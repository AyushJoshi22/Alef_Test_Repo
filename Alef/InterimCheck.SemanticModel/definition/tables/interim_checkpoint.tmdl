			        RETURN MAXX(FILTER(StudentGrowthTable, interim_checkpoint[fle_student_dw_id] = CurrentStudent && [class_dw_id] = CurrentClass), interim_checkpoint[ic_order])),
				interim_checkpoint[ic_order] = [MaxAttempt]-1
				),AVERAGE([StudentScore])
