			                    interim_checkpoint[ic_order] = EARLIER(interim_checkpoint[ic_order]) - 1 &&
			                    NOT ISBLANK(interim_checkpoint[fle_student_dw_id])
			                )
			            )
			        ),
			        interim_checkpoint[ic_order] = [MaxICClass] &&
			        [AvgScores] = [PrevScore] &&
			        NOT ISBLANK([PrevScore]) &&
			        [PrevICClass] = [MaxICClass] - 1
